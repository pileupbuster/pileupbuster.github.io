#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules/jsesc/bin/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules/jsesc/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/node_modules:/mnt/c/Users/DJDaithi/Documents/node_modules:/mnt/c/Users/DJDaithi/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules/jsesc/bin/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules/jsesc/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/node_modules:/mnt/c/Users/DJDaithi/Documents/node_modules:/mnt/c/Users/DJDaithi/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/DJDaithi/Documents/Source/pileupbuster.github.io/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
